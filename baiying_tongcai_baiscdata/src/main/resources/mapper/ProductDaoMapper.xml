<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.btc.dao.ProductDao">

    <resultMap id="BaseResultMap" type="com.btc.main.Product">
        <result column="fid" jdbcType="INTEGER" property="id" />
        <result column="fname" jdbcType="VARCHAR" property="name" />
        <result column="fnumber" jdbcType="VARCHAR" property="number" />
        <result column="ftype" jdbcType="VARCHAR" property="type" />
    </resultMap>

    <select id="getProductList"  resultMap="BaseResultMap" parameterType="java.util.Map">
        select * from  t_product
    </select>
</mapper>